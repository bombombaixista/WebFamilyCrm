@{
    ViewData["Title"] = "Cadastro";
}

<h2>Cadastro</h2>

<form method="post" asp-action="Cadastro">
    <label>Usuário:</label>
    <input type="text" name="username" />
    <br />
    <label>Email:</label>
    <input type="text" name="email" />
    <br />
    <label>Senha:</label>
    <input type="password" name="senha" />
    <br />
    <button type="submit">Cadastrar</button>
</form>
